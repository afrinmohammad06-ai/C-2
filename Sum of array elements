#include <iostream>
using namespace std;

int main() {
    int n, a[50], sum = 0;

    cout << "Enter number of elements: ";
    cin >> n;

    cout << "Enter elements:\n";
    for (int i = 0; i < n; i++) {
        cin >> a[i];
        sum += a[i];
    }

    cout << "Sum of array elements = " << sum;
    return 0;
}
